{
  "extends": "airbnb-base",
  "env": {
      "es6": true,
      "browser": true,
      "jest": true
      },
  "rules": {
      "no-console": "off",
      "max-classes-per-file": "off",
      "no-restricted-syntax": [
      "error",
      "LabeledStatement",
      "WithStatement"
      ]
  },
  "parserOptions": {
    "ecmaVersion": 13
  }
}